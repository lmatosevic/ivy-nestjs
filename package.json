{
  "name": "ivy-nestjs",
  "version": "0.2.7",
  "description": "A library of useful building components for NestJS web application",
  "author": "Luka Matosevic",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "create-resource": "./cli/create-resource.js"
  },
  "maintainers": [
    {
      "name": "Luka Matosevic",
      "email": "lukamatosevic5@gmail.com",
      "web": "https://lukamatosevic.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Lujo5/ivy-nestjs.git"
  },
  "bugs": {
    "url": "https://github.com/Lujo5/ivy-nestjs/issues"
  },
  "homepage": "https://npmjs.com/package/ivy-nestjs",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nestjs",
    "web",
    "api",
    "graphql",
    "auth",
    "logging",
    "config",
    "mongoDB",
    "sql",
    "typeorm"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "npm run copy-prepare",
    "copy-prepare": "node ./tools/copy.js && node ./tools/prepare.js",
    "format": "prettier --write \"lib/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{lib,test}/**/*.ts\" --fix",
    "compodoc": "npx compodoc -p tsconfig.json src",
    "test": "jest --no-cache --forceExit --detectOpenHandles --coverage",
    "prepack": "npm run build",
    "pack": "cd ./dist && npm pack --pack-destination ../build",
    "prerelease": "npm run build",
    "release": "standard-version -a",
    "postrelease": "git commit -am \"chore: update version\" && git push --follow-tags origin main",
    "prepublish": "npm run copy-prepare",
    "publish": "cd ./dist && npm publish"
  },
  "dependencies": {
    "@casl/ability": "^5.4.3",
    "@nestjs/apollo": "^10.0.8",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/bull": "0.6.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/graphql": "^10.0.8",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "^1.0.0",
    "@nestjs/mongoose": "^9.0.0",
    "@nestjs/passport": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/terminus": "^8.0.0",
    "@nestjs/typeorm": "^8.1.2",
    "apollo-server-express": "^3.6.7",
    "axios": "^0.26.1",
    "bcrypt": "^5.0.1",
    "bull": "^4.8.4",
    "class-transformer": "^0.5.0",
    "class-validator": "^0.13.2",
    "crypto-js": "^4.1.1",
    "dotenv": "16.0.1",
    "file-type": "^16.5.3",
    "google-auth-library": "^7.14.0",
    "graphql": "^15.8.0",
    "helmet": "5.0.2",
    "lodash": "^4.17.21",
    "mongodb": "^3.7.3",
    "mongoose": "^6.2.1",
    "multer": "^1.4.5-lts.1",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "standard-version": "9.5.0",
    "swagger-ui-express": "^4.3.0",
    "type-fest": "2.16.0",
    "typeorm": "^0.3.7",
    "uuid": "^8.3.2",
    "winston": "^3.6.0",
    "winston-daily-rotate-file": "4.7.1",
    "yargs": "^17.3.1"
  },
  "devDependencies": {
    "@compodoc/compodoc": "1.1.19",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^3.15.8",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.172",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/pluralize": "^0.0.29",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "esbuild": "0.14.44",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest": "^27.0.6",
    "jest-junit": "^13.1.0",
    "jest-mock-process": "^1.4.1",
    "prettier": "^2.6.2",
    "supertest": "^6.1.3",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5",
    "webpack": "^5.50.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "reflect-metadata": "^0.1.12"
  },
  "engines": {
    "node": ">=16.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.(spec|test)\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ]
  },
  "standard-version": {
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "chore",
        "hidden": true
      },
      {
        "type": "docs",
        "hidden": true
      },
      {
        "type": "style",
        "hidden": true
      },
      {
        "type": "refactor",
        "hidden": true
      },
      {
        "type": "perf",
        "hidden": true
      },
      {
        "type": "test",
        "hidden": true
      }
    ],
    "skip": {
      "commit": true
    }
  }
}
