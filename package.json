{
  "name": "ivy-nestjs",
  "version": "0.6.5",
  "description": "A library of useful building components for NestJS web application",
  "author": "Luka Matosevic",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "create-resource": "./cli/create-resource.js"
  },
  "maintainers": [
    {
      "name": "Luka Matosevic",
      "email": "lukamatosevic5@gmail.com",
      "web": "https://lukamatosevic.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lmatosevic/ivy-nestjs.git"
  },
  "bugs": {
    "url": "https://github.com/lmatosevic/ivy-nestjs/issues"
  },
  "homepage": "https://npmjs.com/package/ivy-nestjs",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nestjs",
    "web",
    "api",
    "rest",
    "graphql",
    "auth",
    "logging",
    "config",
    "nosql",
    "mongodb",
    "mongoose",
    "sql",
    "typeorm",
    "redis",
    "queue",
    "mail",
    "storage",
    "templating",
    "caching"
  ],
  "scripts": {
    "clean": "rimraf dist coverage reports build",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "npm run copy-prepare",
    "copy-prepare": "node ./tools/copy.js && node ./tools/prepare.js",
    "format": "prettier --write \"lib/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{lib,test}/**/*.ts\" --fix",
    "compodoc": "npx compodoc -p tsconfig.json src",
    "test": "jest --no-cache --forceExit --detectOpenHandles --coverage",
    "prepack": "npm run build",
    "pack": "cd ./dist && npm pack --pack-destination ../build",
    "prerelease": "npm run build",
    "release": "standard-version -a",
    "postrelease": "git commit -am \"chore: update version\" && git push --follow-tags origin main",
    "prepublish": "npm run copy-prepare",
    "publish": "cd ./dist && npm publish"
  },
  "dependencies": {
    "@casl/ability": "^6.5.0",
    "@nestjs/apollo": "^12.0.0",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/bull": "^10.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.1.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/graphql": "^12.0.0",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/mapped-types": "^2.0.0",
    "@nestjs/mongoose": "^10.0.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.0.0",
    "@nestjs/terminus": "^10.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "@xmldom/xmldom": "^0.8.10",
    "apollo-server-express": "^3.12.0",
    "axios": "^1.5.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.20.0",
    "bull": "^4.11.0",
    "cache-manager": "^5.2.0",
    "cache-manager-fs-hash": "^1.0.0",
    "cache-manager-ioredis": "^2.1.0",
    "class-transformer": "^0.5.0",
    "class-validator": "^0.14.0",
    "crypto-js": "^4.1.1",
    "dotenv": "^16.3.0",
    "file-type": "^16.5.4",
    "flatted": "^3.2.7",
    "generate-api-key": "^1.0.2",
    "glob": "^10.3.10",
    "google-auth-library": "^8.1.1",
    "graphql": "^16.8.0",
    "handlebars": "^4.7.0",
    "handlebars-helpers": "^0.10.0",
    "helmet": "^7.0.0",
    "inline-css": "^4.0.0",
    "ioredis": "^5.3.0",
    "lodash": "^4.17.0",
    "mongodb": "^5.2.0",
    "mongoose": "^7.6.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.0",
    "object-hash": "^3.0.0",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pluralize": "^8.0.0",
    "reflect-metadata": "^0.1.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "sib-api-v3-sdk": "^8.4.0",
    "standard-version": "^9.5.0",
    "type-fest": "^4.6.0",
    "typeorm": "^0.3.17",
    "uuid": "^9.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^10.2.0",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.7",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^4.10.0",
    "@types/cache-manager": "^4.0.4",
    "@types/crypto-js": "^4.1.1",
    "@types/express": "^4.17.20",
    "@types/inline-css": "^3.0.1",
    "@types/jest": "^29.5.6",
    "@types/lodash": "^4.14.200",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.8.9",
    "@types/nodemailer": "^6.4.13",
    "@types/object-hash": "^2.2.1",
    "@types/passport-jwt": "^3.0.11",
    "@types/passport-local": "^1.0.37",
    "@types/pluralize": "^0.0.29",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.6",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "cross-env": "^7.0.0",
    "esbuild": "^0.19.0",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-mock-process": "^2.0.0",
    "prettier": "^3.0.0",
    "supertest": "^6.2.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.9.0",
    "webpack": "^5.89.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "reflect-metadata": "^0.1.0"
  },
  "engines": {
    "node": ">=16.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.(spec|test)\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "reporters": [
      "default",
      "jest-junit"
    ],
    "coverageReporters": [
      "cobertura",
      "text"
    ],
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./reports",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/test"
    ]
  },
  "jest-junit": {
    "outputDirectory": "./reports",
    "outputName": "junit.xml"
  },
  "standard-version": {
    "types": [
      {
        "type": "feat",
        "section": "Features"
      },
      {
        "type": "fix",
        "section": "Bug Fixes"
      },
      {
        "type": "chore",
        "hidden": true
      },
      {
        "type": "docs",
        "hidden": true
      },
      {
        "type": "style",
        "hidden": true
      },
      {
        "type": "refactor",
        "hidden": true
      },
      {
        "type": "perf",
        "hidden": true
      },
      {
        "type": "test",
        "hidden": true
      }
    ],
    "skip": {
      "commit": true
    }
  }
}
